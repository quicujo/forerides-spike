{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/desktop/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*",
    "storage.*"
  ],
  "modes": {
    "window": {
      "title": "foreriders-app",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/desktop/public/icon.png"
    }
  },
  "cli": {
    "binaryName": "foreriders-app",
    "resourcesPath": "/desktop/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/desktop/public/neutralino.js",
    "frontendLibrary": {
      "patchFile": "/desktop/public/index.html",
      "devUrl": "http:localhost:3000"
    },
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0"
  }
}